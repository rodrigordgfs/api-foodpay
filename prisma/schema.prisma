generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Restaurant {
  id          String   @id @default(uuid())
  name        String
  image       String
  latitude    Float
  longitude   Float
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
